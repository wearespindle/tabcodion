{
    "name": "tabcordions",
    "version": "0.1.7",
    "description": "A minimal tab plugin that acts as accordions on smaller devices. With optional Jquery support.",
    "author": {
        "name": "Spindle",
        "url": "https://wearespindle.com"
    },
    "contributors": [
        {
            "name": "Jeroen van Veen",
            "email": "jeroen@wearespindle.com"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/wearespindle/tabcordions"
    },
    "bugs": {
        "url": "https://github.com/wearespindle/tabcordions/issues"
    },
    "files": [
        "index.js",
        "sass/*.scss"
    ],
    "analyze": true,
    "license": "MIT",
    "scripts": {
        "build-js": "browserify example/index.js | babel --presets es2015 | uglifyjs > example/tabcordion.js; lessc styles.less > example/styles.css",
        "watch-js": "watchify example/index.js -o './node_modules/.bin/babel > example/tabcordion.js'",
        "watch-css": "catw -c 'lessc -' '*.less' -o example/styles.css -v"
    },
    "devDependencies": {
        "babel-cli": "^6.9.0",
        "babel-preset-es2015": "^6.9.0"
    },
    "dependencies": {
        "query-string": "^4.2.0"
    },
    "keywords": [
        "form",
        "tabs",
        "widgets"
    ]
}
